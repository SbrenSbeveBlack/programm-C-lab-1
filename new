#include <stdio.h>
#include <math.h>
int main()
{   double d = 0;
    while (d<=0)
    {
    int i;
    double a,b,c;
    printf("\nВпишите a,b,c через пробел:\n");
    scanf("%lf",&a);
    scanf("%lf",&b);
    scanf("%lf",&c);
    d = pow(b,2) - (4*a*c);
    float a1;
    double x1 = (-b + sqrt(d))/(2*a);
    double x2 = (-b - sqrt(d))/(2*a);
    printf("d = %lf \n", d);
    if (d > 0){
    printf("x1 = %lf x2 = %lf",x1,x2);
    }
    if (d == 0){
    printf("x1 = %lf", x1);
    }
    if (d < 0){
    printf("kornei net");
    
